<?xml version="1.0" encoding="utf-8"?>
<font-family
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!--light-->
    <font
        android:font="@font/square_light"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/square_light"
        app:fontStyle="normal"
        app:fontWeight="500" />

    <!--regular-->
    <font
        android:font="@font/square_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/square_regular"
        app:fontStyle="normal"
        app:fontWeight="500" />

    <!--bold-->
    <font
        android:font="@font/square_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/square_bold"
        app:fontStyle="normal"
        app:fontWeight="1000" />

    <!--ebold-->
    <font
        android:font="@font/square_ebold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/square_ebold"
        app:fontStyle="normal"
        app:fontWeight="1000" />
</font-family>